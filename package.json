{
	"name": "@thomasravnholt/umbracoclient",
	"version": "0.1.1",
	"license": "MIT",
	"description": "A JavaScript client for interacting with the Umbraco Content Delivery API.",
	"repository": {
		"type": "git",
		"url": "https://github.com/ThomasRavnholt/Umbraco-Client"
	},
	"keywords": [
		"umbraco",
		"umbraco client",
		"umbraco delivery api",
		"cms",
		"content delivery",
		"api client",
		"headless cms",
		"umbraco headless",
		"content management",
		"javascript"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.4",
		"@sveltejs/package": "^2.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"publint": "^0.1.9",
		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@thomasravnholt/umbracoclient": "^0.0.2"
	}
}
